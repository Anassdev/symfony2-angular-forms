<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />

    <title>Symfony2, AngularJs, Forms</title>

    {% stylesheets '@bootstrap_css' 'bundles/acmedemo/css/style.css' %}
        <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}

</head>

<body>

{% block body %}
    <div class="content">
        {% block content %}{% endblock %}
    </div>

    {% javascripts
        '@jquery_js'
        '@angular_js'
        '@angular_animate_js'
        '@bootstrap_js'
        output="js/libraries.js"
    %}
        <script src="{{ asset_url }}"></script>
    {% endjavascripts %}

    {% block js %}{% endblock %}
{% endblock %}
